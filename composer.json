{
    "name": "madboy-developers/laravel-auth",
    "description": "This is laravel-auth package by MadBoy Developers.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Hardik Modha",
            "email": "hardik.d.modha@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^8.0",
        "spatie/laravel-package-tools": "^1.7.0",
        "spatie/laravel-permission": "^4.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0|^5.0|^6.0",
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "MadBoyDevelopers\\LaravelAuth\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "MadBoyDevelopers\\LaravelAuth\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MadBoyDevelopers\\LaravelAuth\\LaravelAuthServiceProvider"
            ]
        }
    },
    "prefer-stable": true
}
